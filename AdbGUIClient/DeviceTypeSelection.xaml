<Window x:Class="AdbGUIClient.DeviceTypeSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdbGUIClient"
        mc:Ignorable="d"
        Title="DeviceTypeSelection"
        Height="80"
        Width="240"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Horizontal"
                    HorizontalAlignment="Center">
            <Button Width="120" Click="Android_Click">Android</Button>
            <Button Width="120" Click="IOS_Click">iOS</Button>
        </StackPanel>
    </Grid>
</Window>
